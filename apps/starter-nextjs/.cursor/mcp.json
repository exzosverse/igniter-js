{
  "mcpServers": {
    "igniter-mcp": {
      "command": "npx",
      "args": [
        "-y",
        "@igniter-js/mcp-server@latest"
      ],
      "env": {
        "GOOGLE_API_KEY": "",
        "GITHUB_TOKEN": ""
      }
    },
    "browser": {
      "command": "npx @playwright/mcp@latest",
      "env": {},
      "args": []
    },
    "shadcn": {
      "command": "npx",
      "args": ["shadcn@latest", "mcp"]
    }
  }
}
