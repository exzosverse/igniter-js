    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "baseUrl": "src",
    "paths": {
      "@/*": ["./*"]
    },
    "types": ["https://deno.land/x/deno@v1.46.3/types.d.ts"]
  }
}
