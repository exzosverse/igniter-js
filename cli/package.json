{
  "name": "@igniter-js/cli",
  "version": "1.0.0",
  "description": "CLI for Igniter.js type-safe client generation",
  "main": "dist/index.js",
  "bin": {
    "igniter": "./bin/igniter"
  },
  "files": [
    "dist",
    "bin"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "igniter",
    "cli",
    "typescript",
    "code-generation",
    "api"
  ],
  "author": "Felipe Barcelos <felipebarcelospro@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "commander": "^11.0.0",
    "chokidar": "^3.6.0"
  },
  "peerDependencies": {
    "@igniter-js/core": ">=0.1.0"
  },
  "devDependencies": {
    "typescript": "^5.6.3",
    "tsup": "^8.3.5",
    "@types/node": "^22.9.0"
  }
} 